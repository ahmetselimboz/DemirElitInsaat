
  
    <div class="margin">
      
    </div>
    <section>
      <div class="cat-panel">
        <div class="cat-title-area">
         
          <h2 ><%- title %></h2>

          <div class="part-line " >
            <hr class="part-underline"  id="cat-part-underline"/>
            <div class="part-img" id="cat-part">
              <img src="/frontend/assets/Just_Logo.png" alt="" />
            </div>
            <hr class="part-underline" id="cat-part-underline" />
          </div>
        </div>
        
   

        <div class="cat-contain">
          <% for( let index = 0; index < apart.length; index++ ) { %>
            <div class="cat-item ">
              <img class="img-fluid-p" src="https://drive.google.com/thumbnail?id=<%- apart[index].images[0].id %>&sz=w3000" alt="" />
              <% if (apart[index].last_date != null) { %>
                <%var lastdate = apart[index].last_date.toISOString().substr(0, 10);%>
                <%const datenow = new Date().toISOString().substr(0, 10);%>
          
                <%var date1 = new Date(datenow);%>
                <%var date2 = new Date(lastdate);%>
                  
               
                <% var Difference_In_Time = date2.getTime() - date1.getTime();%>
                  
          
                <%var Difference_In_Days = Difference_In_Time / (1000 * 3600 * 24);%>
                <div class="day-area">
                  <h3><%- Difference_In_Days  %> gün</h3>
                </div>
              <% } %>
            
              <div class="cat-text">
                <h2><%- apart[index].project_name %></h2>
              </div>
              <div class="cat-overlay">
                <div class="cat-text-hover">
                  <a href="/daire-detay/<%- apart[index].id %>" class="cat-btn-incele">İncele</a>
                  <h4><%- apart[index].adress %></h4>
                </div>
              </div>
            </div>
          <% } %>
       
      
        </div>
      </div>
    </section>