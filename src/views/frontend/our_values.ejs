<div class="about-top">
  <img src="/frontend/assets/1132471.jpg" alt="" />
  <div class="about-text-top">
    <div class="about-bottom-text">
      <div class="about-bottom-alt">
        <h4>KURUMSAL</h4>
      </div>
      <hr class="about-bottom-line" />
      <h6>Değerlerimiz</h6>
    </div>
  </div>
</div>

<section>
  <div class="about-panel">
    <div class="about-area">
      <div class="house-title-area">
        <hr class="house-underline js-scroll slide-left" />
        <h3 class="text-uppercase js-scroll slide-left">Değerlerimiz</h3>
      </div>
      <div class="deg-area">
        <% for( let index = 0; index < our.length; index++ ) { %>
        <div class="deg-col">
          <div class="deg-title-area">
            <h3 class="text-uppercase js-scroll slide-left">
              <i class="bx bx-chevron-right"></i><%- our[index].our_title %>
            </h3>
          </div>
          <div class="deg-text js-scroll slide-left">
            <h5><%- our[index].our_text %></h5>
          </div>
        </div>
        <% } %>
      </div>
    </div>
  </div>
</section>
